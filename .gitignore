.idea/
*.iml
misc.xml
modules.xml
profiles_settings.xml
vcs.xml
workspace.xml

# Compiled C extension
*.so

# Distribution / packaging
dist/
build/
*.egg-info/
*.egg

# Environmental variables
.env

# macOS specific files
.DS_Store

# Windows specific files
Thumbs.db
